<!doctype html>
<html lang="en">

<head>
    <title>NEORYX HISTORIC</title>
    <meta charset="utf-8" />
    <link rel="stylesheet" href="./styles/prettify.css" />
    <link rel="stylesheet" href="./styles/report.css" />
    <link rel="stylesheet" href="./styles/styles.css" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
</head>

<body>

<!--        -------------------------------------------------------------------------------------->
<!--        ---------------------------------------   Header   ----------------------------------->
<!--        -------------------------------------------------------------------------------------->


<div class="header">
    <div class="header-left">
        <div class="main-title">
            <h1>NEORYX HISTORIC</h1>
        </div>
        <div class="clearfix">
            <div class='fl pad1y space-right2 subtitle'>
                <span class="strong">PROJECT</span>
            </div>
            <div class='fl pad1y subtitle-files'>
                <span>Files : </span>
                <span class="strong">{{project.files}}</span>
            </div>
            <div class='fl pad1y subtitle-others'>
                <span>Methods : </span>
                <span class="strong">{{project.methods}}</span>
            </div>
            <div class='fl pad1y subtitle-others'>
                <span>Functions : </span>
                <span class="strong">{{project.functions}}</span>
            </div>
            <div class='fl pad1y subtitle-statements'>
                <span>Statements : </span>
                <span class="strong">{{project.statements}}</span>
            </div>
        </div>
        <div class="clearfix">
            <div class='fl pad1y space-right2 subtitle'>
                <span class="strong">REGRESSIONS</span>
            </div>
            <div class='fl pad1y subtitle-files'>
                <span class="strong">{{regressions}}</span>
            </div>
        </div>
        <div class="clearfix">
            <div class='fl pad1y space-right2 subtitle'>
                <span class="strong"><a href="./dashboard.html"></a></span>
            </div>
        </div>
    </div>
    <div class="header-right">
    </div>
</div>


<div class="mainContainer">
    <div class="subfolders">
        <div class="clearfix cognitive-subtitle">
            <div class='fl pad1y space-right2'>
                <span class="strong">Global coverage</span>
            </div>
        </div>

        <table class="array-report short-table">
            <thead>
            <tr>
                <th style="width: 35%"></th>
                {{#each fullDates}}
                    <th>{{this.date}}<br>{{this.time}}</th>
                {{/each}}
            </tr>
            </thead>
            <tr>
                <td>Code coverage</td>
                {{#each globalCoverage}}
                    <td class="{{this.color}} center">
                        {{this.text}}
                    </td>
                {{/each}}
            </tr>
        </table>
    </div>

    <div class="subfolders">
        <div class="clearfix cognitive-subtitle">
            <div class='fl pad1y space-right2'>
                <span class="strong">Coverage historic</span>
            </div>
        </div>
        <table class="array-report short-table">
            <thead>
            <tr>
                <th style="width: 20%">File</th>
                <th style="width: 15%">Method</th>
                {{#each fullDates}}
                    <th>{{this.date}}<br>{{this.time}}</th>
                {{/each}}
            </tr>
            </thead>
            {{#each historicRows}}
                <tr>
                    <td>{{this.fileName}}</td>
                    <td>{{this.methodName}}</td>
                    {{#each this.coveragesColor}}
                        <td class="{{this.color}} center">
                            {{this.text}}
                        </td>
                    {{/each}}
                </tr>
            {{/each}}
        </table>
    </div>
</div>


</body>

</html>
