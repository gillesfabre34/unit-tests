<!doctype html>
<html lang="en">

<head>
    <title>NEORYX HISTORIC</title>
    <meta charset="utf-8" />
    <link rel="stylesheet" href="./styles/prettify.css" />
    <link rel="stylesheet" href="./styles/report.css" />
    <link rel="stylesheet" href="./styles/styles.css" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
</head>

<body>

<!--        -------------------------------------------------------------------------------------->
<!--        ---------------------------------------   Header   ----------------------------------->
<!--        -------------------------------------------------------------------------------------->


<div class="header">
    <div class="header-left">
        <div class="main-title">
            <h1>NEORYX HISTORIC</h1>
        </div>
        <div class="clearfix">
            <div class='fl pad1y space-right2 subtitle'>
                <span class="strong">REGRESSIONS</span>
            </div>
            <div class='fl pad1y subtitle-files'>
                <span class="strong">{{regressions}}</span>
            </div>
        </div>
        <div class="clearfix">
            <div class='fl pad1y space-right2 subtitle'>
                <span class="strong"><a href="./dashboard.html"> <- Dashboard</a></span>
            </div>
        </div>
    </div>
    <div class="header-right">
    </div>
</div>


<div class="mainContainer">
    <div class="subfolders">
        <div class="clearfix cognitive-subtitle">
            <div class='fl pad1y space-right2'>
                <span class="strong">Coverage historic</span>
            </div>
        </div>
        <table class="array-report short-table">
            <thead>
            <tr>
                <th style="width: 30%">File / Method</th>
                {{#each datesArray}}
                    <td>{{this}}</td>
                {{/each}}
            </tr>
            </thead>
            {{#each methodsCoverage}}
                <tr>
                    <td>{{this.name}}</td>
                    {{#each methodsCoverage}}
                        <td class="{{this.color}}">
                            {{this.value}}
                        </td>
                    {{/each}}
                </tr>
            {{/each}}
        </table>
    </div>
</div>


<!--        -------------------------------------------------------------------------------------->
<!--        -----------------------------------   Scripts-----   --------------------------------->
<!--        -------------------------------------------------------------------------------------->



<script src="./styles/Chart.js"></script>

</body>

</html>
